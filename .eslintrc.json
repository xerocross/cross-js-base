{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc" : "off",
        "linebreak-style" : "off",
        "indent": ["error", 4],
        "space-before-function-paren" : ["error", "always"],
        "key-spacing": ["error", { "beforeColon": true, "afterColon": true, "mode": "strict" }],
        "comma-dangle": ["error", "never"],
        "quotes": ["error", "double"],
        "max-len": ["error", { "code": 200 }]
    }
}
